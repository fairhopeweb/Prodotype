{
  "name": "prodotype",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "author": "lexoyo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silexlabs/Prodotype.git"
  },
  "engines": {
    "node": "8.10.x",
    "npm": "5.6.x"
  },
  "scripts": {
    "prepare": "npm run build:editor",
    "prepublishOnly": "git clean -n && read -p \"Continue? (CTRL+C to abort)?\" && git clean -fd && npm run build:editor",
    "build": "npm run build:editor && npm run build:templates",
    "watch": "nodemon --watch 'src' --exec 'npm run build'",
    "reload": "reload -v -b -d pub/ -p 8080",
    "build:editor": "browserify src/Prodotype.js -o pub/prodotype.js -t [ babelify ] && cat `node_modules ejs`/ejs/ejs.min.js >> pub/prodotype.js",
    "build:templates": "bin/prodotype sample/ pub/sample",
    "start": "http-server ."
  },
  "bin": {
    "prodotype": "bin/prodotype"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babelify": "^10.0.0",
    "brace": "^0.11.1",
    "browserify": "^16.2.3",
    "ejs": "^2.6.1",
    "js-yaml": "^3.13.1",
    "node_modules-path": "^2.0.2",
    "nodemon": "^1.19.0",
    "react": "^16.8.6",
    "react-ace": "^6.5.0",
    "react-dom": "^16.8.6",
    "react-tap-event-plugin": "^3.0.3",
    "reload": "^3.0.1"
  }
}
