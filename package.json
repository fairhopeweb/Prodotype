{
  "name": "prodotype",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/silexlabs/Prodotype.git"
  },
  "engines": {
    "node": "8.10.x",
    "npm": "5.6.x"
  },
  "scripts": {
    "postinstall": "npm run build:editor",
    "build": "npm run build:editor && npm run build:templates",
    "watch": "nodemon --watch 'src' --exec 'npm run build'",
    "reload": "reload -v -b -d pub/ -p 8080",
    "build:editor": "browserify src/Prodotype.js -o pub/prodotype.js -t [ babelify ] && cat `node_modules ejs`/ejs/ejs.min.js >> pub/prodotype.js",
    "build:templates": "bin/prodotype sample/ pub/sample",
    "start": "http-server ."
  },
  "bin": {
    "prodotype": "bin/prodotype"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "brace": "^0.9.1",
    "browserify": "^13.0.1",
    "ejs": "^2.5.5",
    "js-yaml": "^3.6.1",
    "node_modules-path": "^2.0.0",
    "react": "^15.2.0",
    "react-ace": "^3.5.0",
    "react-dom": "^15.2.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "http-server": "^0.9.0",
    "nodemon": "^1.5.0-alpha4",
    "reload": "^1.0.1"
  }
}
