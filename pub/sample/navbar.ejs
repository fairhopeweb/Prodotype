<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand"
        <% if(onbrand) { %>onclick="<%= onbrand %>"<% } %>
      ><%- brand %></a>
    </div>

    <div class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <% for(var idx in items) { %>
        <li class="<% if(items[idx].active) { %> active<% } %>">
          <a
            <% if(items[idx].onclick) { %>onclick="<%= items[idx].onclick %>"<% } %>
            <% if(items[idx].href) { %>href="<%= items[idx].href %>"<% } %>
          ><%= items[idx].text %></a>
        </li>
        <% } %>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <% for(var idx in links) { %>
        <li>
          <a
            <% if(links[idx].onclick) { %>onclick="<%= links[idx].onclick %>"<% } %>
            <% if(links[idx].href) { %>href="<%= links[idx].href %>"<% } %>
          ><%= links[idx].text %></a>
        </li>
        <% } %>
      </ul>
    </div>
  </div>
</nav>
